{
    "name": "FEmbed-Lwip",
    "keywords": "FEmbed, lwip, network",
    "description": "FEmbed Lwip wrap library for lwip",
    "authors":
    {
        "name": "Gene Kong",
        "email": "gyx_edu@qq.com",
        "url": "https://github.com/FastEmbedded/FEmbed-Lwip.git"
    },
    "repository":
    {
        "type": "git",
        "url": "https://github.com/FastEmbedded/FEmbed-Lwip.git"
    },
    "version": "0.1.0",
    "frameworks": "esp8266-rtos-sdk",
    "platforms": "custom8266",
    "build": {
        "flags": [
            "-DUSE_ESPRESSIF8266=1",
            "-DMQTTCLIENT_QOS1=1",
            "-DMQTTCLIENT_QOS2=1"
        ],
        "includeDir": "src,paho/MQTTClient/src,paho/MQTTPacket/src",
        "srcDir": "src,paho/MQTTPacket/src",
        "srcFilter":[
            "+<*.c>",
            "+<*.cpp>",
            "+<*.h>"
        ]
    },
    "dependencies": [ 
    {
        "name": "FEmbed-OSlib",
        "version": ">=0.1.0",
        "platforms": "custom8266"
    },
    {
        "name": "FEmbed-Arduino",
        "version": ">=0.1.0",
        "platforms": "custom8266"
    },
    {
        "name": "FEmbed-ChainConfig",
        "version": ">=0.1.0",
        "platforms": "custom8266"
    }
    ]
}